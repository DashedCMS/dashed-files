{
    "name": "dashed/dashed-files",
    "description": "This is my package dashed-files",
    "keywords": [
        "Dashed",
        "laravel",
        "dashed-files"
    ],
    "homepage": "https://github.com/Dashed-DEV/dashed-files",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@dashed.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ralphjsmit/laravel-filament-media-library": "^4.0-beta4",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "filament/upgrade": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.63",
        "orchestra/testbench": "^9",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Dashed\\DashedFiles\\": "src",
            "Dashed\\DashedFiles\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dashed\\DashedFiles\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "ralphjsmit/packages": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashed\\DashedFiles\\DashedFilesServiceProvider",
                "Dashed\\DashedFiles\\DashedFilesEventServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ]
}
